{
    "name": "cocur/slugify-symfony-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for original cocur/slugify, which Converts a string into a slug.",
    "keywords": ["slug", "slugify"],
    "license": "MIT",
    "authors": [
        {
            "name": "Benedikt Lenzen",
            "email": "benedikt.lenzen@bl-websolutions.de"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/config": "~2.4|~3.0|^4.0|^4.1",
        "symfony/dependency-injection": "~2.4|~3.0|^4.0|^4.1",
        "symfony/http-kernel": "~2.4|~3.0|^4.0|^4.1",
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8.36|~5.2",
        "twig/twig": "~1.26|~2.0"
    },
    "autoload": {
        "psr-4": {"Cocur\\SlugifySymfonyBundle\\": ""},
        "exclude-from-classmap": [
            "Tests/"
        ]
    }
}
